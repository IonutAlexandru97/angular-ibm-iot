    "jsonwebtoken": "^8.4.0",
    "morgan": "^1.9.1",
    "multer": "^1.4.1",
    "pg-promise": "^8.5.5",
    "schemats": "^3.0.3",
    "sequelize": "^4.42.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/bcrypt-nodejs": "0.0.30",
    "@types/express": "^4.16.1",
    "@types/jsonwebtoken": "^8.3.0",
    "@types/morgan": "^1.7.35",
    "@types/multer": "^1.3.7",
    "@types/node": "^11.9.4",
    "@types/pg-promise": "^5.4.3",
    "@types/uuid": "^3.4.4",
    "nodemon": "^1.18.10",
    "ts-node": "^8.0.2",
    "typescript": "^3.3.3"
